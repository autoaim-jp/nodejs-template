{
  "source": {
    "include": ["router", "lib", "app.js", "setting.js", "browserServerSetting.js"],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "recurse": true,
    "destination": "./doc/jsdoc/html/server/",
    "template": "node_modules/tui-jsdoc-template"
  }
}

